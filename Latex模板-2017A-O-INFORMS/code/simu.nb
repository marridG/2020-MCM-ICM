(* Interest Function *)

Benefit =  Income - Cost/((1 - P1) (1 - P2) (1 - P3) (1 - P4));

(* Benefit Functions *)
Income = recycle + funding + payment;

(* P1 : Ground Handling Risk Functions *)

(* P2 : Launch Risk Functions *)

(* P3 : Space Risk Functions *)

P3 = k2*F[d, h, i, t, S]*(volume)^(2/3)* T;

(* P4 : Re-entry Risk Functions *)

(* Flux for Orbital Debris *)

F[d, h, i, t, S] := k * v[h, S] * V[i]*(F1[d]*g1[t] + F2[d]*g2[t]); 

(* Subfunctions *)
v[h, S] := v1[h, S]/(v1[h, S] + 1);
v1[h, S] := 10^(h/200 - S/140 - 1.5);
F1[d] := 1.05*10^(-5)*d^(-2.5);
F2[d] := 7.0*10^10*(d + 700)^(-6);
g1[t] := (1 + 2*p)^(t - 1985);
g2[t] := (1 + p)^(t - 1985);
 
